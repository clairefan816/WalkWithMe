<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="#F7D1BA"
    tools:context=".userlog.LogoutFragment">

    <Button
        android:id="@+id/yes_logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="103dp"
        android:backgroundTint="#E8A746"
        android:layout_marginTop="307dp"
        android:onClick="onClick"
        android:text="Yes" />

    <Button
        android:id="@+id/not_logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="222dp"
        android:layout_marginTop="307dp"
        android:backgroundTint="#1FAB89"
        android:onClick="onClick"
        android:text="No" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="206dp"
        android:layout_height="67dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="102dp"
        android:layout_marginTop="204dp"
        android:fontFamily="@font/baloo"
        android:text="Are you sure you want log out?"
        android:textSize="24sp" />
</RelativeLayout>